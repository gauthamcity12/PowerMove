<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="io.github.gauthamcity12.powermove" >

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="AIzaSyAO4Dn9wKCq-8fhiMTqnTk-tkL02-KQXQY" />

        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service
            android:name=".GetLocation"
            android:enabled="true" />

        <activity
            android:name=".MainOptionsActivity"
            android:label="@string/title_activity_main_options" >
        </activity>
        <activity
            android:name=".ChoiceActivity"
            android:label="@string/title_activity_choice" >
        </activity>
    </application>

</manifest>
